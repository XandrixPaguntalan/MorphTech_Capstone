<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background='#3c3939'
    tools:context=".number8">

    <Button
        android:id="@+id/butt31"
        android:layout_width="338dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="168dp"
        android:backgroundTint="#fff"
        android:text="norovirus"
        android:textColor="#d67c31"
        app:layout_constraintEnd_toEndOf="@+id/textView209"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView209"
        app:layout_constraintTop_toTopOf="@+id/textView209" />

    <Button
        android:id="@+id/butt32"
        android:layout_width="338dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="228dp"
        android:backgroundTint="#fff"
        android:text="hiv"
        android:textColor="#d67c31"
        app:layout_constraintEnd_toEndOf="@+id/textView209"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView209"
        app:layout_constraintTop_toTopOf="@+id/textView209" />

    <Button
        android:id="@+id/butt33"
        android:layout_width="338dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="288dp"
        android:backgroundTint="#fff"
        android:text="whooping cough"
        android:textColor="#d67c31"
        app:layout_constraintEnd_toEndOf="@+id/textView209"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView209"
        app:layout_constraintTop_toTopOf="@+id/textView209" />

    <TextView
        android:id="@+id/textView209"
        android:layout_width="338dp"
        android:layout_height="wrap_content"
        android:text="_______ is a very contagious virus that causes vomiting and diarrhea. Anyone can get infected and sick with norovirus. You can get norovirus from having direct contact with an infected person, consuming contaminated food or water, touching contaminated surfaces and then putting your unwashed hands in your mouth."
        android:textColor="#fff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.116" />
</androidx.constraintlayout.widget.ConstraintLayout>