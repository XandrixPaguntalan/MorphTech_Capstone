        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="160dp"
        android:text="- Night Sweats"
        android:textColor="#fff"
        app:layout_constraintStart_toStartOf="@+id/textView17"
